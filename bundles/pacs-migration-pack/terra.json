{
  "id": "pacs-migration-pack",
  "type": "bundle",
  "name": "PACS Migration Pack",
  "version": "1.0.0",
  "description": "Complete toolkit for migrating from legacy PACS systems to TerraFusion",
  "license": "MIT",
  "author": "TerraFusion Team",
  "homepage": "https://terrafusion.io/bundles/pacs-migration-pack",
  "repository": "https://github.com/terrafusion/pacs-migration-pack",
  "dependencies": [
    "terrafusion-core@1.0.0",
    "dicom-parser@3.0.1",
    "image-converter@2.1.0"
  ],
  "settings": {
    "maxConcurrentJobs": 5,
    "batchSize": 100,
    "logLevel": "info",
    "cleanupPolicy": {
      "preserveOriginals": true,
      "tempFilesRetentionDays": 7
    }
  },
  "requirements": {
    "memory": "2Gi",
    "cpu": "1.0",
    "storage": "10Gi"
  },
  "metadata": {
    "category": "migration",
    "priority": "high",
    "tags": ["pacs", "migration", "dicom", "healthcare"],
    "supportedSystems": [
      "ClearCanvas",
      "Horos",
      "OsiriX",
      "DCM4CHEE",
      "Orthanc"
    ]
  }
}