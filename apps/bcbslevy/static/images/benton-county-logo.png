<svg width="200" height="200" viewBox="0 0 200 200" xmlns="http://www.w3.org/2000/svg">
  <style>
    .mountain { fill: #267b9c; }
    .sun { fill: #ffb74d; }
    .text { fill: #ffffff; font-family: Arial, sans-serif; }
  </style>
  
  <!-- Sun -->
  <circle class="sun" cx="50" cy="60" r="25" />
  
  <!-- Sun rays -->
  <polygon class="sun" points="50,25 55,15 45,15" />
  <polygon class="sun" points="50,95 55,105 45,105" />
  <polygon class="sun" points="15,60 5,55 5,65" />
  <polygon class="sun" points="85,60 95,55 95,65" />
  <polygon class="sun" points="25,35 15,25 20,40" />
  <polygon class="sun" points="75,35 85,25 80,40" />
  <polygon class="sun" points="25,85 15,95 20,80" />
  <polygon class="sun" points="75,85 85,95 80,80" />
  
  <!-- Mountains -->
  <polygon class="mountain" points="0,150 60,80 100,120 160,50 200,100 200,150" />
  
  <!-- Text -->
  <text class="text" x="100" y="175" text-anchor="middle" font-size="20" font-weight="bold">BENTON COUNTY</text>
</svg>
