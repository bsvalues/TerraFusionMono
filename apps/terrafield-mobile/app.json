{
  "expo": {
    "name": "TerraField",
    "slug": "terrafield",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.terrafusion.terrafield"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.terrafusion.terrafield",
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow TerraField to use your location to show your position on the map and for parcel data.",
          "locationAlwaysPermission": "Allow TerraField to use your location in the background to support field operations.",
          "locationWhenInUsePermission": "Allow TerraField to use your location while the app is in use."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "terrafield"
      }
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/terrafield"
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    }
  }
}