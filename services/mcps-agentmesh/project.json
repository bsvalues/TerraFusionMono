{
  "name": "mcps-agentmesh",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "services/mcps-agentmesh/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/node:webpack",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/services/mcps-agentmesh",
        "main": "services/mcps-agentmesh/src/main.ts",
        "tsConfig": "services/mcps-agentmesh/tsconfig.json"
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false
        }
      }
    },
    "serve": {
      "executor": "@nx/node:node",
      "options": {
        "buildTarget": "mcps-agentmesh:build"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["services/mcps-agentmesh/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "services/mcps-agentmesh/jest.config.ts"
      }
    },
    "pack": {
      "executor": "@terrafusion/nx-pack:pack",
      "outputs": ["{options.outputPath}/mcps-agentmesh"],
      "options": {
        "outputPath": "dist/pack",
        "includeFiles": [
          "terra.json",
          "README.md",
          "LICENSE",
          "dist/**/*",
          "charts/**/*",
          "scripts/**/*"
        ],
        "excludeFiles": [
          "node_modules/**/*",
          "**/*.test.ts",
          "**/*.spec.ts",
          "**/jest.config.ts"
        ],
        "generateChecksums": true,
        "validateSchema": true,
        "generateSBOM": true,
        "sbomFormat": "cyclonedx"
      }
    }
  },
  "tags": []
}