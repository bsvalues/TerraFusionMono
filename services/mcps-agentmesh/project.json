{
  "name": "mcps-agentmesh",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "services/mcps-agentmesh/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/services/mcps-agentmesh",
        "main": "services/mcps-agentmesh/src/main.ts",
        "tsConfig": "services/mcps-agentmesh/tsconfig.json",
        "assets": ["services/mcps-agentmesh/src/assets"]
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["services/mcps-agentmesh/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "services/mcps-agentmesh/jest.config.ts"
      }
    },
    "docker": {
      "executor": "@nx/js:node",
      "options": {
        "buildTarget": "mcps-agentmesh:build",
        "dockerFile": "services/mcps-agentmesh/Dockerfile"
      }
    },
    "pack": {
      "executor": "@terrafusion/nx-pack:pack",
      "options": {
        "outputPath": "dist/pack",
        "includeFiles": ["terra.json", "README.md", "LICENSE", "charts/**/*"],
        "generateChecksums": true
      }
    }
  },
  "tags": ["service", "mcps", "agent"]
}