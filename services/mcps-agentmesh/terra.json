{
  "id": "mcps-agentmesh",
  "type": "service",
  "name": "MCPS Agent Mesh",
  "version": "1.0.0",
  "description": "Distributed agent network for medical imaging data exchange",
  "maintainer": "TerraFusion Team <support@terrafusion.io>",
  "license": "PROPRIETARY",
  "docs": "https://docs.terrafusion.io/mcps-agentmesh",
  "tags": ["mcps", "agent", "mesh", "dicom", "medical-imaging"],
  "infra": {
    "helmChart": "charts/mcps-agentmesh",
    "requirements": {
      "cpu": "1",
      "memory": "1Gi",
      "storage": "5Gi"
    },
    "ports": [
      {
        "name": "http",
        "containerPort": 8080,
        "servicePort": 80,
        "protocol": "TCP"
      },
      {
        "name": "mesh",
        "containerPort": 9090,
        "servicePort": 9090,
        "protocol": "TCP"
      }
    ]
  },
  "dependencies": [
    "dicom-converter",
    "nats-server",
    "postgresql"
  ],
  "config": {
    "logLevel": {
      "type": "string",
      "description": "Logging level for the service",
      "default": "info",
      "enum": ["debug", "info", "warn", "error"]
    },
    "meshPort": {
      "type": "number",
      "description": "Port for mesh communication",
      "default": 9090,
      "minimum": 1024,
      "maximum": 65535
    },
    "agentCount": {
      "type": "number",
      "description": "Number of agent replicas to run",
      "default": 3,
      "minimum": 1,
      "maximum": 10
    },
    "maxConcurrentTransfers": {
      "type": "number",
      "description": "Maximum number of concurrent transfers",
      "default": 5,
      "minimum": 1,
      "maximum": 20
    },
    "securityLevel": {
      "type": "string",
      "description": "Security level for data encryption",
      "default": "high",
      "enum": ["standard", "high", "extreme"]
    }
  }
}