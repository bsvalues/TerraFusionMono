{
  "id": "mcps-agentmesh",
  "type": "service",
  "name": "MCPS Agent-Mesh",
  "version": "1.0.0",
  "description": "Multi-Core Processing System Agent-Mesh Service for TerraFusion",
  "license": "MIT",
  "author": "TerraFusion Team",
  "homepage": "https://terrafusion.io/services/mcps-agentmesh",
  "repository": "https://github.com/terrafusion/mcps-agentmesh",
  "dependencies": [
    "terrafusion-core@1.0.0",
    "mesh-agent@2.1.0",
    "processor-core@3.0.0"
  ],
  "settings": {
    "maxAgents": 12,
    "meshTopology": "distributed",
    "messageRateLimit": 1000,
    "retryPolicy": {
      "maxRetries": 3,
      "backoffFactor": 1.5
    }
  },
  "requirements": {
    "memory": "1Gi",
    "cpu": "0.5",
    "storage": "2Gi"
  },
  "ports": [
    {
      "name": "http",
      "port": 8080,
      "protocol": "TCP"
    },
    {
      "name": "mesh",
      "port": 9090,
      "protocol": "TCP"
    }
  ],
  "metadata": {
    "category": "processing",
    "priority": "high",
    "tags": ["agent", "mesh", "processing", "distributed"]
  }
}