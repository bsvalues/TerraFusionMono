{
  "all": true,
  "check-coverage": true,
  "branches": 80,
  "lines": 80,
  "functions": 80,
  "statements": 80,
  "include": ["src/**/*.ts", "src/**/*.tsx"],
  "exclude": ["**/node_modules/**", "**/test/**", "**/dist/**", "**/*.d.ts"],
  "reporter": ["html", "text", "text-summary", "lcov"],
  "extension": [".ts", ".tsx"],
  "cache": true,
  "temp-dir": "./coverage/.nyc_output",
  "report-dir": "./coverage"
}